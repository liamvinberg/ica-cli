{
  "name": "ica-cli",
  "version": "0.1.0",
  "description": "ICA grocery list skill wrapper",
  "environment": {
    "ICA_CURRENT_ACCESS_TOKEN": "<required-or-provided-by-secret-store>",
    "ICA_CURRENT_REFRESH_TOKEN": "<optional>"
  },
  "commands": {
    "auth_status": "ica --json auth status",
    "auth_login_agentic_start": "ica --json auth login --agentic --user \"{{user}}\"",
    "auth_login_agentic_complete": "ica --json auth login --agentic --user \"{{user}}\" --callback-url \"{{callback_url}}\"",
    "auth_import_session": "ica --json auth session import --session-id \"{{session_id}}\"",
    "auth_import_token": "ica --json auth token import --access-token \"{{access_token}}\" --refresh-token \"{{refresh_token}}\"",
    "list_all": "ica --json list ls",
    "list_items": "ica --json list items --list-name \"{{list_name}}\"",
    "add_item": "ica --json list add \"{{item}}\" --list-name \"{{list_name}}\"",
    "search_products": "ica --json products search \"{{query}}\" --store-id \"{{store_id}}\""
  }
}
