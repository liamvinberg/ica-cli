{
  "name": "ica-cli",
  "version": "0.1.0",
  "description": "ICA grocery list skill wrapper",
  "environment": {
    "ICA_CURRENT_ACCESS_TOKEN": "<required-or-provided-by-secret-store>",
    "ICA_CURRENT_REFRESH_TOKEN": "<optional>"
  },
  "commands": {
    "auth_status": "ica --json auth status",
    "list_all": "ica --json list ls",
    "add_item": "ica --json list add \"{{item}}\" --list-name \"{{list_name}}\"",
    "search_products": "ica --json products search \"{{query}}\" --store-id \"{{store_id}}\""
  }
}
